{
    "name": "attentrack/theme",
    "description": "AttenTrack WordPress Theme",
    "type": "wordpress-theme",
    "require": {
        "firebase/php-jwt": "^6.0",
        "php": ">=7.4",
        "razorpay/razorpay": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AttenTrack\\": "inc/"
        }
    },
    "replace": {
        "rmccue/requests": "*"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
